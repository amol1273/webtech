df

df1=df['Science']
mean=statistics.mean(df1)
mean

df1=df['Science']
median=statistics.median(df1)
median

df1=df['Science']
mode=statistics.mode(df1)
mode

sqr=math.sqrt(25)
sqr  #gives squareroot

log=math.log(25)
log

fact=math.factorial(5)
fact

square = math.pow(9, 2)
square

square=np.square(df['English'])  
square

version= sys.version
version
